{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Features": {
        "Chatbot": true
    },
    "Chatbot": {
        "Provider": "OpenAI",
        "Model": "gpt-3.5-turbo",
        "Temperature": 0.2,
        "MaxOutputTokens": 450,
        "ApiKey": "ENV:OPENAI_API_KEY",
        "SystemPromptPath": "AppData/prompts/system.txt"
    }

}
